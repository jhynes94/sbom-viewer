{
    "routes": [
        {
            "route": "/*",
            "serve": "/build/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "/build/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*"
        ]
    }
}
